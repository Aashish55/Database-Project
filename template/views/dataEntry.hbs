    <div class="container">
        <form id="form" action="/submitfamilyinformation" method="post">
            <h3>Family Information</h3>
            <input placeholder="House Number" name="housenumber" type="number" tabindex="2" required>
            <input placeholder="Number of Male" name="numberofmale" type="number" tabindex="4" required>
            <input placeholder="Number of Female" name="numberoffemale" type="number" tabindex="4" required>
            <input placeholder="Number of children" name="numberofchildren" type="number" tabindex="4" required>
            <input placeholder="Number of Earning Member" name="earningnumber" type="number" tabindex="4" required>
            <input placeholder="Number of Dependent Member" name="dependentnumber" type="number" tabindex="4" required>
            <input placeholder="Number of Disabled Member" name="disablednumber" type="number" tabindex="4" required>
            <input placeholder="Number of Literate Member" name="literatenumber" type="number" tabindex="4" required>
            <input placeholder="Permanent Adress" name="paddress" type="text" tabindex="4" required>
            <button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
            </fieldset>
        </form>
    </div>
    <div class="container">
        <form id="form" action="submithouseinformation" method="post">
            <h3>House Information</h3>
            <fieldset>
            <input placeholder="House Number" name="housenumber" type="number" tabindex="1" required autofocus>
            <input placeholder="Owner" name="owner" type="text" tabindex="2" required>
            <input placeholder="Age of Housing" name="type" type="number" tabindex="4" required>
            <input placeholder="Insurance" name="insurance" type="text" tabindex="4" required>
            <input placeholder="Source of Light" name="sourceoflight" type="text" tabindex="4" required>
            <input placeholder="Source of Fuel" name="sourceoffuel" type="text" tabindex="4" required>
            <button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
            </fieldset>
        </form>
    </div>
    <div class="container">
        <form id="form" action="/submithousestructure" method="post">
            <h3>House Structure</h3>
            <fieldset>
            <input placeholder="House Number" name="housenumber" type="number" tabindex="1" required autofocus>
            <input placeholder="Type" type="text" name="type" tabindex="2" required>
            <input placeholder="Roofing Type" name="roofingtype" type="text" tabindex="4" required>
            <input placeholder="EarthQuake Resistant" name="earthquake" type="text" tabindex="4" required>
            <input placeholder="Number of Storey" type="number" name="numberofstorey" tabindex="4" required>
            <input placeholder="Number of Rooms" type="number" name="numberofrooms" tabindex="4" required>
            <input placeholder="Latrine" name="latrine" type="text" tabindex="4" required>
            <button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
            </fieldset>
        </form>
    </div>
    <div class="container">
        <form id="form" action="/submitarea" method="post">
            <h3>Land Information</h3>
            <fieldset>
            <input placeholder="House Number" name="housenumber" type="number" tabindex="1" required autofocus>
            <input placeholder="Type" name="type" type="text" tabindex="2" required>
            <input placeholder="Area" name="area" type="number" tabindex="4" required>
            <button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
            </fieldset>
        </form>
    </div>
    <div class="container">
        <form id="form" action="/submitluxury" method="post">
            <h3>Luxury</h3>
            <fieldset>
            <input placeholder="House Number" name="housenumber" type="number" tabindex="1" required autofocus>
            <input placeholder="Vehicles" name="vehicles" type="text" tabindex="2" required>
            <input placeholder="Internet" name="internet" type="text" tabindex="4" required>
            <input placeholder="CellPhone" name="cellphone" type="text" tabindex="4" required>
            <button name="submit" type="submit" id="contact-submit" data-submit="...Sending">Submit</button>
            </fieldset>
        </form>
    </div>
</body>

