        <form id="form" action="" method="post">
            <h3>Family Information</h3>
            <fieldset>
                <input placeholder="House Number" type="text" tabindex="2" required>
            </fieldset>
            <fieldset>
                <input placeholder="Number of Male" type="text" tabindex="4" required>
            </fieldset>
            <fieldset>
                <input placeholder="Number of Female" type="text" tabindex="4" required>
